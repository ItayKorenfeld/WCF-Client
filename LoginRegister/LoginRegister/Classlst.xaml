<Page x:Class="LoginRegister.Classlst"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LoginRegister"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Classlst">
    <Grid Background="FloralWhite">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"> </RowDefinition>
            <RowDefinition Height="6*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock x:Name="txt1" HorizontalAlignment="Center" FontSize="30" Grid.Row="0" Foreground="Turquoise" Text=" Class List For House Keeper" />
     

        <Button Background="Azure" Grid.Row="2" x:Name="button10"  Content="Back" Grid.Column="0" Width="75" Height="50" HorizontalAlignment="Center" Click="button1_Click"/>
        <Grid Grid.Row="1" x:Name="g"/>
    </Grid>
</Page>
